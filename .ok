clean:   rm src/menhir_parser.{automaton,conflicts,ml,mli} 2>/dev/null || true
explain: ok clean && menhir -v --table src/menhir_parser.mly
build:   ok clean && dune build @all

run:     ./_build/default/src/compiler.exe
venv:    source ../e2e-test-runner/venv/bin/activate 
test:    ok venv && python ../e2e-test-runner/runner.py
debug:   ok venv && textual run --dev ../e2e-test-runner/runner.py
format:  ocamlformat --inplace ./src/*.ml
