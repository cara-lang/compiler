module IO {

  pure : a -> IO[a]
  bind : IO[a] -> (a -> IO[b]) -> IO[b]

  println : a -> IO[()]
  inspect : a -> IO[()]

  toString        : a -> String
  toInspectString : a -> String

  ask : String -> IO[String]

  readFile  : String -> IO[Maybe[String]]
  writeFile : {filename: String, content: String} -> IO[()]
  
}
